{
  "name": "@yaegaki/unity-http-server",
  "version": "0.0.2",
  "displayName": "unity-http-server",
  "description": "A simple HTTP server optimized for serving Unity Web builds",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "bin": {
    "unity-http-server": "dist/server.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "npm run build && node dist/server.js",
    "dev": "ts-node src/server.ts"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "keywords": ["unity", "web", "webgl", "server", "http"],
  "author": "yaegaki",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yaegaki/unity-http-server.git"
  },
  "homepage": "https://github.com/yaegaki/unity-http-server",
  "bugs": {
    "url": "https://github.com/yaegaki/unity-http-server/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "@google-cloud/storage": "^7.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/node": "^20.5.0",
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5"
  }
}
